<h1 class="top-bar form-header">Log In</h1>

<div class="row form">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<div class="field small-12 columns">
    <%= f.label :email %>
    <%= f.email_field :email, autofocus: true %>
  </div>

  <div class="field small-12 columns">
    <%= f.label :password %>
    <%= f.password_field :password, autocomplete: "off" %>
  </div>

  <div class="actions button alert" type="submit">
    <button class="create-button small-12 offset-6 columns">
    Sign In
  </button>
<% end %>
</div>
